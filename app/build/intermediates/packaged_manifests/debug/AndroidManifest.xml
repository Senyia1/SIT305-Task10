<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.game.carorder"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="32" />

    <application
        android:name="com.game.carorder.App"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.CarOrder" >
        <activity
            android:name="com.game.carorder.activity.OrderDetailActivity"
            android:exported="false" />
        <activity
            android:name="com.game.carorder.activity.AddOrderStep2Activity"
            android:exported="false" />
        <activity
            android:name="com.game.carorder.activity.AddOrderStep1Activity"
            android:exported="false" />
        <activity
            android:name="com.game.carorder.activity.SignActivity"
            android:exported="true" />
        <activity
            android:name="com.game.carorder.activity.LoginActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.game.carorder.activity.MainActivity"
            android:exported="true" />
    </application>

</manifest>